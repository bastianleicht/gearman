{
    "name": "brianlmoon/net_gearman",
    "homepage": "https://brian.moonspot.net/GearmanManager",
    "description": "PHP daemon for managing gearman workers",
    "keywords": ["gearman","net_gearman","gearman-manager"],
    "type": "library",
    "license": "BSD-2-Clause",
    "config": {
        "discard-changes": true,
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Brian Moon",
            "homepage": "https://brian.moonspot.net/"
        }
    ],
    "support": {
        "issues": "https://github.com/brianlmoon/net_gearman/issues"
    },
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "classmap": [ "Net/Gearman" ]
    },
    "autoload-dev": {
        "psr-4": {
            "Net\\Gearman\\Tests\\": "tests/"
        }
    }
}
